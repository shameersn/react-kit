{
  "name": "react-kit",
  "version": "1.0.0",
  "description": "Ready to go React project",
  "main": "index.js",
  "author": "Thomas Thiebaud",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build:prod",
    "build:prod": "webpack --config config/webpack.prod.js",
    "clean": "rimraf dist/ yarn-error.log npm-debug.log",
    "lint": "tslint --project .",
    "lint:fix": "npm run lint -- --fix",
    "message:precommit": "echo 'Pre-commit checks...' && exit 0",
    "start": "npm run start:dev",
    "start:dev": "webpack-dev-server --open --config config/webpack.dev.js",
    "start:prod": "npm run build:prod && http-server --cors dist/"
  },
  "pre-commit": [
    "message:precommit",
    "lint"
  ],
  "dependencies": {
    "bulma": "0.6.2",
    "normalize.css": "8.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-hot-loader": "4.0.0-beta.21",
    "react-loadable": "5.3.1",
    "react-redux": "5.0.6",
    "react-router-config": "1.0.0-beta.4",
    "react-router-dom": "4.2.2",
    "react-spinkit": "3.0.0",
    "redux": "3.7.2",
    "redux-saga": "0.16.0"
  },
  "devDependencies": {
    "@types/node": "9.4.6",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "@types/react-loadable": "5.3.3",
    "@types/react-redux": "5.0.15",
    "@types/react-router-config": "1.0.6",
    "@types/react-router-dom": "4.2.4",
    "@types/react-spinkit": "3.0.3",
    "autoprefixer": "7.2.6",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-dynamic-import": "6.18.0",
    "css-loader": "0.28.9",
    "cssnano": "3.10.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "http-server": "0.11.1",
    "node-sass": "4.7.2",
    "postcss-loader": "2.1.0",
    "pre-commit": "1.2.2",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.20.1",
    "ts-loader": "3.5.0",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "uglifyjs-webpack-plugin": "1.1.8",
    "url-loader": "0.6.2",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  }
}
